                <RowDefinition Height="72"/>
                <RowDefinition Height="5"/>
                <RowDefinition Height="72"/>
                <RowDefinition Height="5"/>
                <RowDefinition Height="72"/>
                <RowDefinition Height="5"/>
                <RowDefinition Height="72"/>
                <RowDefinition Height="5"/>
                <RowDefinition Height="72"/>
            </Grid.RowDefinitions>
        </Grid>
        <Border Grid.Column="1" Grid.Row="0" Margin="5,3,5,0">
            <Components:UCMainStore x:Name="ucMainStore"/>
        </Border>
    </Grid>
</UserControl>
